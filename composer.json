{
    "name": "apility/bring-client",
    "description": "Bring API Client for the Netflex SDK",
    "type": "library",
    "license": "MIT",
    "require": {
        "netflex/api": "^3.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Apility\\Bring\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
          "providers": [
            "Apility\\Bring\\Providers\\BringServiceProvider"
          ],
          "aliases": {
            "Bring": "Apility\\Bring\\Facades\\Bring"
          }
        }
    }
}